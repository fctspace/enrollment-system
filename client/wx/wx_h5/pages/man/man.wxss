.kefu{
  background: linear-gradient(to right, blue, pink);
  height: 100vh;
  display: flex;
  align-items: center;
  justify-content: center;
}
.my-kefu {
  display: inline-block;
  width:686rpx;
  height: 96rpx;
  background:rgba(0,204,255,1);
  border-radius: 16rpx;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  line-height: 96rpx;
  margin-top: 64rpx;
}
.n {
  height: 90vh;
  display: flex;
  align-items: center;
  justify-content: center;
  flex-direction: column
}
.n > image {
  width: 480rpx;
  height: 384rpx;
}
.n > view {
  width:686rpx;
  height: 96rpx;
  background:rgba(0,204,255,1);
  border-radius: 16rpx;
  text-align: center;
  font-size: 36rpx;
  color: #fff;
  line-height: 96rpx;
  margin-top: 64rpx;
}